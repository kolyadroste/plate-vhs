{
    "name": "atomicplan/plate-vhs",
    "version": "0.5.0",
    "type": "typo3-cms-extension",
    "description": "provides plate specific viewhelpers",
    "keywords": ["TYPO3 CMS", "Integrators", "Site Extension", "Site Package", "TypoScript"],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Kolya von Droste zu Vischering",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "typo3/cms-core": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Atomicplan\\PlateVhs\\": "Classes/"
        }
    },
    "replace": {
        "typo3-ter/atomicplan/plate-vhs": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "plate_vhs"
        }
    }
}